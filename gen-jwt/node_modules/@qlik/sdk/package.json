    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "husky": "8.0.3",
    "jszip": "3.10.1",
    "start-server-and-test": "1.15.2",
    "ts-jest": "29.0.3",
    "ts-jest-resolver": "2.0.0",
    "ts-node": "10.9.1",
    "typedoc": "0.23.23",
